# Database configuration
POSTGRES_DB=ifrs9db
POSTGRES_USER=ifrs9user
# Postgres database configuration
POSTGRES_PASSWORD=ifrs9password
POSTGRES_HOST=db
POSTGRES_PORT=5432
# Full database connection URL (overrides individual settings if set)
DATABASE_URL=postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@${POSTGRES_HOST}:${POSTGRES_PORT}/${POSTGRES_DB}
# AI configuration
# Choose AI agent: "claude" or "codex"
AI_AGENT=codex
# API key for Anthropic Claude (when using Claude agent)
CLAUDE_API_KEY=
# API key for OpenAI Codex/GPT (when using Codex agent)
OPENAI_API_KEY=
